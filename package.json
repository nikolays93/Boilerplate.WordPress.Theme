{
  "name": "boilerplate.wordpress.theme",
  "description": "",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "@todo": [
    "Think about inlcude @wordpress/scripts plugin"
  ],
  "scripts": {
    "~lint:scss": "wp-scripts lint-style 'sass/**/*.scss'",
    "~lint:js": "wp-scripts lint-js 'js/*.js'",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist assets/_source style.css.map",
    "dev": "gulp --silent",
    "watch": "gulp",
    "build": "gulp build --production",
    "build::images": "gulp rebuild::images",
    "tunnel": "gulp --tunnel --silent",
    "install": "gulp install"
  },
  "devDependencies": {
    "dir-archiver": "^1.1.1",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-debug": "^4.0.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-watch": "^5.0.1",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "imagemin-zopfli": "^6.0.0",
    "map-stream": "^0.0.7",
    "merge-stream": "^2.0.0",
    "webpack": "^4.43.0",
    "webpack-stream": "^5.2.1",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "animatewithsass": "^3.2.1",
    "bootstrap": "^4.5.2",
    "cleave.js": "^1.6.0",
    "font-awesome": "^4.7.0",
    "hamburgers": "^1.1.3",
